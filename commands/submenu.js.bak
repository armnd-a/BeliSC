exports.run = {
   usage: ['submenu'],
   async: async (m, {
      client,
      args,
      isPrefix
   }) => {
      try {
         const dial = args[0] ? args[0] : 1
         if (dial == 1) return client.reply(m.chat, submenu1(isPrefix), m)
         if (dial == 2) return client.reply(m.chat, submenu2(isPrefix), m)
         if (dial == 3) return client.reply(m.chat, submenu3(isPrefix), m)
         if (dial == 4) return client.reply(m.chat, submenu4(isPrefix), m)
         if (dial == 5) return client.reply(m.chat, submenu5(isPrefix), m)
         if (dial == 6) return client.reply(m.chat, submenu6(isPrefix), m)
         if (dial == 7) return client.reply(m.chat, submenu7(isPrefix), m)
         if (dial == 8) return client.reply(m.chat, submenu8(isPrefix), m)
         if (dial == 9) return client.reply(m.chat, submenu9(isPrefix), m)
         if (dial ==10) return client.reply(m.chat, submenu10(isPrefix), m)    
      } catch (e) {
         console.log(e)
      }
   },
   error: false,
   cache: true,
   location: __filename
}

